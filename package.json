{
  "name": "timeline",
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:js": "tsc",
    "build:css": "tailwindcss -i ./src/style.css -o ./dist/style.css",
    "dev:js": "npm run build:js -- --watch",
    "dev:css": "npm run build:css -- --watch"
  },
  "dependencies": {
    "axios": "1.2.2",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "tailwindcss": "3.2.6"
  }
}
